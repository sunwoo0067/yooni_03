# Simple Product Collector 환경 설정

# 데이터베이스 설정 (테스트용 SQLite)
DATABASE_URL=sqlite:///./simple_collector.db

# API 설정
API_TITLE=Simple Product Collector
API_VERSION=1.0.0

# 로그 설정
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# 수집 설정
COLLECTION_BATCH_SIZE=100
OWNERCLAN_CACHE_SIZE=5000

# 도매처 API 키 (실제 사용 시 입력)
ZENTRADE_API_KEY=
OWNERCLAN_API_KEY=
DOMEGGOOK_API_KEY=

# 업로드 설정
UPLOAD_DIR=uploads
MAX_FILE_SIZE=10485760