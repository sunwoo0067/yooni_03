{
  "name": "yoonidashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "build:ios:dev": "cd ios && xcodebuild -workspace YooniDashboard.xcworkspace -scheme YooniDashboard -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
    "build:ios:prod": "cd ios && xcodebuild -workspace YooniDashboard.xcworkspace -scheme YooniDashboard -configuration Release -sdk iphoneos -derivedDataPath ios/build",
    "build:android:dev": "cd android && ./gradlew assembleDebug",
    "build:android:prod": "cd android && ./gradlew assembleRelease",
    "deploy:ios": "fastlane ios release",
    "deploy:android": "fastlane android release",
    "deploy:beta": "fastlane beta",
    "codepush:ios": "appcenter codepush release-react -a YourOrg/YooniDashboard-iOS -d Production",
    "codepush:android": "appcenter codepush release-react -a YourOrg/YooniDashboard-Android -d Production",
    "codepush:all": "npm run codepush:ios && npm run codepush:android",
    "e2e:ios": "detox test --configuration ios.sim.debug",
    "e2e:android": "detox test --configuration android.emu.debug",
    "pods": "cd ios && pod install",
    "clean": "watchman watch-del-all && rm -rf node_modules/ && npm cache clean --force && npm install",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean"
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.0",
    "@react-native-async-storage/async-storage": "^1.19.5",
    "@react-native-community/netinfo": "^11.1.0",
    "@react-native-firebase/analytics": "^18.7.3",
    "@react-native-firebase/app": "^18.7.3",
    "@react-native-firebase/crashlytics": "^18.7.3",
    "@react-native-firebase/messaging": "^18.7.3",
    "@react-native-firebase/perf": "^18.7.3",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^1.9.7",
    "@tanstack/react-query": "^5.17.9",
    "axios": "^1.6.5",
    "dayjs": "^1.11.10",
    "i18next": "^23.7.16",
    "react": "18.2.0",
    "react-i18next": "^14.0.0",
    "react-native": "0.72.7",
    "react-native-biometrics": "^3.0.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-code-push": "^8.1.0",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.11.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-keychain": "^8.1.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^2.11.0",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^14.1.0",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.45",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "babel-jest": "^29.2.1",
    "detox": "^20.18.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|@react-navigation|react-native-vector-icons|react-native-reanimated|react-native-mmkv|react-native-biometrics|@notifee/react-native|react-native-chart-kit|react-native-svg|react-native-gesture-handler|react-native-screens|react-native-safe-area-context|react-native-modal|react-native-linear-gradient|react-native-fast-image|react-native-keychain|react-native-device-info|react-native-config|react-native-code-push|@react-native-firebase|@react-native-async-storage|@react-native-community)/)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "./jest.setup.js"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.ts"
    ]
  }
}