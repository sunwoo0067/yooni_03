[run]
source = app
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */migrations/*
    */alembic/*
    */config.py
    */__init__.py

[report]
precision = 2
show_missing = True
skip_covered = False

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    except ImportError:
    pass

[html]
directory = tests/coverage_html_report

[json]
output = tests/coverage.json