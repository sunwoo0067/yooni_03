{
  "name": "yooni-dropshipping-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:integrated": "vite --config configs/vite.integrated.config.ts --mode development",
    "dev:operational": "vite --config configs/vite.operational.config.ts --mode development",
    "dev:personal": "vite --config configs/vite.personal.config.ts --mode development",
    "build": "vite build",
    "build:check": "tsc --noEmit && vite build",
    "build:integrated": "vite build --config configs/vite.integrated.config.ts",
    "build:operational": "vite build --config configs/vite.operational.config.ts",
    "build:personal": "vite build --config configs/vite.personal.config.ts",
    "build:all": "npm run build && npm run build:integrated && npm run build:operational",
    "build:clean": "rimraf dist && npm run build",
    "build:analyze": "npm run build && npm run analyze",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "preview:personal": "vite preview --config configs/vite.personal.config.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "check": "npm run type-check && npm run lint && npm run format:check && npm run test:run",
    "fix": "npm run lint:fix && npm run format",
    "prepare": "cd .. && husky install frontend/.husky",
    "pre-commit": "lint-staged",
    "analyze": "vite-bundle-analyzer"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^5.14.19",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.14.20",
    "@mui/x-data-grid": "^6.18.3",
    "@mui/x-date-pickers": "^6.18.3",
    "@reduxjs/toolkit": "^2.0.1",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.84.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.2",
    "chart.js": "^4.5.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.539.0",
    "node-fetch": "^3.3.2",
    "notistack": "^3.0.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-grid-layout": "^1.5.2",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^5.1.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.10.3",
    "socket.io-client": "^4.7.2",
    "sonner": "^2.0.7",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@rollup/rollup-win32-x64-msvc": "^4.45.1",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-grid-layout": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.0.11",
    "prettier": "^3.1.1",
    "puppeteer": "^24.15.0",
    "terser": "^5.43.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-bundle-analyzer": "^1.2.0",
    "vitest": "^3.2.4",
    "whatwg-fetch": "^3.6.20"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
